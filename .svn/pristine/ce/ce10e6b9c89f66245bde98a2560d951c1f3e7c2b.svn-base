

body {
	Color: #FFFFFF;
	Font-Family: Verdana, Sans-Serif;
	Font-Size: 8pt;
	Background-Color: #000000;
	background-image:url(Grafik/TextureBlue.jpg); 
}

body.EMail {
	Color: #FFFFFF;
	Font-Family: Verdana, Sans-Serif;
	Font-Size: 8pt;
	Background-Color: #000000;
	background-image:url(Grafik/EMailTextureBlue.jpg); 
}

a:link {
	Color: #FEC516;
}

a:visited {
	Color: #FFE79D;
}

a:hover {
	Color: #FF0000;
}

a img {
  border: 0 none;
}


a.menu:link {
	Color: #ff0000;
	text-decoration: none;
	font-weight: bold;
	font-size: 11px;
	letter-spacing: 0.4em;
}

a.menu:visited {
	Color: #ff0000;
	text-decoration: none;
	font-weight: bold;
	font-size: 11px;
	letter-spacing: 0.4em;
}

a.menu:hover {
	Color: #000000;
	text-decoration: none;
	font-weight: bold;
	font-size: 14px;
	letter-spacing: 0.4em;
}



.Center {
	Text-align: center;
}


.highlight {
	font-size: 14px; 
	font-weight: bold;
	color: #FFEC40;
}


h1 {
	Font-Size: 14pt;
	Font-Style: bold;
	text-align: center;
	Color: #FF2020;
}

h2 {
	Font-Size: 14pt;
	Font-Style: bold;
	text-align: left;
	Color: #FF2020;
}

h3 {
	font-size: 14px; 
	font-weight: bold;
	color: #FFEC40;
}




table.Content {
	width: 800px;
}

table.Title {
	background-image: url(Grafik/Title.png);
	background-repeat: no-repeat;
	background-position: top center;
	width: 800px;
}

table.listing {
	width: 90%;
	border-spacing: 1px;
	background-color: #404040;
	text-align: left;
	vertical-align: top;
}

tr.even {
	background-color: #000000;
}

tr.odd {
	background-color: #202020;
}



td {
	Font-Family: Veranda, Sans-Serif;
	Font-Size: 9pt;
}

td.Title {
	text-align: center;
}

td.Content {
	padding: 20px;
}


td.Head {
	text-align: center;
	border: none;
	margin: 0px;
}

td.Menu {
	text-align: center;
	word-spacing: 20px;
	border-style: none;
/*	Background-Color: #000000;*/
}

td.Future {
	color: #FFFF00;
}



textarea {
	font-family: helvetica, arial, sans-serif;
	background-color: #ffffff;
}

input {
	font-family: helvetica, arial, sans-serif;
	background: #fff;
}


